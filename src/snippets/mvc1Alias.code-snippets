          "\tLocal oView := Nil",
          "",
          "\toView := FwFormView():New()",
          "\toView:SetModel(oModel)",
          "\toView:AddField(\"VIEW_${2:alias}\", oStru${2:alias}, \"${2:alias}MASTER\")",
